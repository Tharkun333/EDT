{% extends 'base.html.twig' %}


{% block Contenu %}
<h3>Liste des cours</h3>
<table class="table table-striped table-hover">
    <thead>
        <tr>
        <th scope="col">Id</th>
        <th scope="col">Heure de debut</th>
        <th scope="col">Heure de fin</th>
        <th scope="col">Type</th>
        <th scope="col">Action</th>
        </tr>
    </thead>
    {% for cour in list %}
        <tbody>
            <tr>
                <th scope="row">{{cour.id}}</th>
                <td>{{cour.toStringDateDebut}}</td>
                <td>{{cour.toStringDateFin}}</td>
                <td>{{cour.type}}</td>
                <td>
                <a  href="#">Editer</a>
                <a  href="#">Supprimer</a>
                
                </td>
            </tr>
        </tbody>
    {% endfor %}
</table>

{% endblock %}

